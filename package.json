{
  "name": "sumconfig-monorepo",
  "version": "0.0.0",
  "description": "Monorepo for sumconfig projects",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/hildjj/sumconfig.git"
  },
  "homepage": "https://github.com/hildjj/sumconfig",
  "scripts": {
    "test": "ava",
    "clean": "rimraf coverage node_modules pkg/*/node_modules pkg/*/coverage",
    "coverage": "c8 ava",
    "docs": "typedoc  --tsconfig tsconfig.json pkg/*/lib/*.js",
    "lint": "eslint . --ext js,cjs,mjs"
  },
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "workspaces": [
    "./pkg/*"
  ],
  "devDependencies": {
    "@cto.af/eslint-config": "^0.1.0",
    "@types/node": "^17.0.8",
    "ava": "^4.0.1",
    "c8": "^7.11.0",
    "eslint": "^8.6.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14.0"
  }
}
